<section-def-macros>
    <def-macro n="f_agr_formal1" npar="1">
<choose>
   <when>

     <test><or>
	     <equal><clip pos="1" side="tl" part="a_pers_nbr"/> <lit-tag v="p2.frm.sg"/></equal>
		 <equal><clip pos="1" side="tl" part="a_pers_nbr"/><lit-tag v="p2.frm.pl"/></equal>
	 </or></test>
  
     <let><clip pos="1" side="tl" part="a_pers_nbr"/><lit-tag v="p2.pl"/></let>
    </when>
</choose>


    </def-macro>


<def-macro n="f_agr_formall" npar="1">
   <choose> 
  <when>
 
     <test><or><equal><clip pos="1" side="tl" part="poss_v"/> <lit-tag v="px2sg.frm"/></equal>
	        <equal><clip pos="1" side="tl" part="poss_v"/><lit-tag v="px2pl.frm"/></equal></or></test>
  
    <let><clip pos="1" side="tl" part="poss_v"/><lit-tag v="px2pl"/></let>
    </when>
</choose>
    </def-macro>


 <def-macro n="p_impf" npar="1">
<choose>
   <when>
          <test>
             <equal>
                <clip pos="1" side="sl" part="prc_impf"/>
                <lit-tag v="prc_impf"/>
              </equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="prc_impf"/>
            <lit-tag v="gna_impf"/>
          </let>
        </when>
</choose>


    </def-macro>
    
  </section-def-macros>
  
  

<section-rules>

<rule comment="REGLA: AOR-1" > <!-- сыйламайды екенсің  -> ödüllendirmezsin -->
      <pattern>
        <pattern-item n="aor"/>
      </pattern>
      <action>
      <call-macro n="f_agr_formal1">
         <with-param pos="1"/>
      </call-macro>
<choose>
   <when>
     
      <test><equal><clip pos="1" side="tl" part="evid"/><lit-tag v="aor.evid"/></equal></test>
                             
        
      <let><clip pos="1" side="tl" part="evid"/><lit-tag v="aor"/></let>
    </when>
</choose>
<let>
<clip pos="1" side="tl" part="a_aspect"/><lit-tag v="aor"/>
</let>
        <let>
          <clip pos="1" side="tl" part="a_pers_nbr"/>
          <concat><lit v="+i"/><lit-tag v="cop.aor"/><clip pos="1" side="tl" part="a_pers_nbr"/></concat>
        </let>
        <out>
          <chunk name="v" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

<rule comment="REGLA: gpr_impf-2" > <!--болашағы -> olacağı  -->
      <pattern>
        <pattern-item n="gpr_impf_subst"/>
      </pattern>
      <action>
     

        
          <let>
          <clip pos="1" side="tl" part="a_fut"/>
          <lit-tag v="gpr_fut"/>
        </let>
       
        <out>
          <chunk name="gpr" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

